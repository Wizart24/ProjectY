<Window x:Class="wpftest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpftest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dtg_Pictures" d:ItemsSource="{d:SampleData ItemCount=5}" Height="NaN" Margin="0,116,0,167"/>
        <TextBox x:Name="txt_Name" HorizontalAlignment="Left" Margin="68,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txt_Password" HorizontalAlignment="Left" Margin="68,54,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btn_Login" Content="Login" HorizontalAlignment="Left" Margin="209,39,0,0" VerticalAlignment="Top" Click="btn_Login_Click"/>
        <TextBlock x:Name="txt_Code" HorizontalAlignment="Left" Margin="264,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="78" Width="476"/>

    </Grid>
</Window>
